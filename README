cd ~
git clone https://github.com/jfhu/conf.git .conf

# Bash: run this line or add "source ~/.conf/bash/bashrc" in existing .bashrc
ln -s .conf/bash/bashrc .bashrc
# then optionally modify ./conf/bash/local/variables.bash

# Tmux
ln -s .conf/tmux.conf .tmux.conf

# Git
ln -s .conf/gitconfig .gitconfig
